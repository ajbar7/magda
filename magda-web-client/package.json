    "flow-bin": "^0.39.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "^1.0.10",
    "redux-mock-store": "^1.2.1",
    "typescript": "2.3.4"
  },
  "dependencies": {
    "d3-drag": "^1.0.1",
    "d3-selection": "^1.0.2",
    "draft-js": "^0.10.1",
    "draft-js-mention-plugin": "TerriaJS/draft-js-mention-plugin#v2.1.0",
    "draft-js-plugins-editor": "2.0.0-rc2",
    "es6-promise": "^4.1.0",
    "fetch-mock": "^5.8.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^0.7.7",

{
  "name": "@magda/web-client",
  "description": "The MAGDA in-browser web front end.",
  "version": "0.0.28-SNAPSHOT",
  "license": "Apache-2.0",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "dev": "npm run start",
    "surge-build": "react-scripts build",
    "build": "npm run compile",
    "compile": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch": "react-scripts build --watch"
  },
  "devDependencies": {
    "@magda/scripts": "^0.0.28-SNAPSHOT",
    "deep-freeze": "0.0.1",
